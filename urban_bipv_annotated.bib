}

@article{dimitriou_bim_nodate,
	title = {{BIM} {ENABLED} {BUILDING} {ENERGY} {MODELLING}: {DEVELOPMENT} {AND} {VERIFICATION} {OF} {A} {GBXML} {TO} {IDF} {CONVERSION} {METHOD}},
	abstract = {As part of the Design4Energy retrofit scenario a methodology is developed that uses Building Information Modelling (BIM) of existing domestic buildings to assess their energy performance using a Building Energy Modelling (BEM) technique. The focus is on the conversion process from gbXML BIM export file to an idf file for EnergyPlusTM. The conversion process is broken down into six steps of progressive addition of idf objects to enable verification. The measured operational data are used to assess the adequacy of the defaults being used.},
	language = {en},
	author = {Dimitriou, Vanda and Firth, Steven K and Hassan, Tarek M and Fouchal, Farid},
	pages = {8},
}

