	year = {2019},
	doi = {10.1007/978-3-030-18456-8_5},
	keywords = {Bioclimatic design measures, Bioclimatic potential, Bioclimatic strategies, Climate adapted buildings, Passive cooling, Passive solar heating, Shading, Thermal insulation},
