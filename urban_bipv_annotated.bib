	number = {1},
	urldate = {2021-01-26},
	journal = {Buildings and Cities},
	author = {Salter, Jonathan and Lu, Yuhao and Kim, Ju Chan and Kellett, Ronald and Girling, Cynthia and Inomata, Fausto and Krahn, Alix},
	month = jul,
	year = {2020},
	note = {Number: 1
Publisher: Ubiquity Press},
	keywords = {buildings, energy demand, energy model, energy policy, neighborhood, urban morphology, ‘what-if’ scenarios},
	pages = {293--307},
}

