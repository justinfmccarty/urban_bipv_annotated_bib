	author = {Ligtenberg, Arend and Bregt, Arnold K. and van Lammeren, Ron},
	month = sep,
	year = {2001},
	keywords = {Cellular automata, Multi-actor decision making, Multi-agent systems, SWARM, Spatial planning, Spatial simulation, \_tablet},
	pages = {21--33},
}

