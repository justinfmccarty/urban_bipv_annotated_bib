	title = {Averages of {Forecasts}: {Some} {Empirical} {Results}},
	volume = {29},
	issn = {0025-1909},
	shorttitle = {Averages of {Forecasts}},
	url = {https://pubsonline-informs-org.ezproxy.library.ubc.ca/doi/abs/10.1287/mnsc.29.9.987},
