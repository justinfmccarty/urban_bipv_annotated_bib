@article{gaur_climate_2019,
	title = {Climate {Data} to {Undertake} {Hygrothermal} and {Whole} {Building} {Simulations} {Under} {Projected} {Climate} {Change} {Influences} for 11 {Canadian} {Cities}},
	volume = {4},
	copyright = {http://creativecommons.org/licenses/by/3.0/},
	url = {https://www.mdpi.com/2306-5729/4/2/72},
	doi = {10.3390/data4020072},
	abstract = {Buildings and homes in Canada will be exposed to unprecedented climatic conditions in the future as a consequence of global climate change. To improve the climate resiliency of existing and new buildings, it is important to evaluate their performance over current and projected future climates. Hygrothermal and whole building simulation models, which are important tools for assessing performance, require continuous climate records at high temporal frequencies of a wide range of climate variables for input into the kinds of models that relate to solar radiation, cloud-cover, wind, humidity, rainfall, temperature, and snow-cover. In this study, climate data that can be used to assess the performance of building envelopes under current and projected future climates, concurrent with 2 \&deg;C and 3.5 \&deg;C increases in global temperatures, are generated for 11 major Canadian cities. The datasets capture the internal variability of the climate as they are comprised of 15 realizations of the future climate generated by dynamically downscaling future projections from the CanESM2 global climate model and thereafter bias-corrected with reference to observations. An assessment of the bias-corrected projections suggests, as a consequence of global warming, future increases in the temperatures and precipitation, and decreases in the snow-cover and wind-speed for all cities.},
	language = {en},
	number = {2},
	urldate = {2020-10-28},
	journal = {Data},
	author = {Gaur, Abhishek and Lacasse, Michael and Armstrong, Marianne},
	month = jun,
	year = {2019},
	note = {Number: 2
Publisher: Multidisciplinary Digital Publishing Institute},
	keywords = {Canada, bias correction, buildings, climate change, climate model, hygrothermal modelling},
	pages = {72},
}

@article{allcott_short-run_2014,
