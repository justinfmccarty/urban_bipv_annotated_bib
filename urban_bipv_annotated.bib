	number = {Special},
	urldate = {2019-05-02},
	journal = {Wood and Fiber Science},
	author = {Wang, J.Y. and Stirling, R and Morris, Paul I. and Taylor, A. and Lloyd, J. and Kirker, G. and Lebow, S. and Mankowski, M. and Barnes, H. M. and Morrell, J. J.},
	month = aug,
