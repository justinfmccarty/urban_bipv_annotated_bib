	pages = {833--858},
}

@article{ghosh_potential_2020-1,
	title = {Potential of building integrated and attached/applied photovoltaic ({BIPV}/{BAPV}) for adaptive less energy-hungry building’s skin: {A} comprehensive review},
	volume = {276},
	issn = {09596526},
